                    {% import "system/macro/form-component/status-indicator.html.twig" as status_indicator %}
                    {% block content %}
                        {{ status_indicator.component({
                            cssClasses: ['bc-justify-end'],
                            errorText: data.status.system.cronjob.message,
                            status: data.status.system.cronjob.success,
                        }, localized) }}
                    {% endblock %}
                {% endembed %}
            {% endblock %}
        {% endembed %}

