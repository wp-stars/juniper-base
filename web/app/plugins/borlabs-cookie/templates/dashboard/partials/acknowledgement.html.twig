{% embed "system/embed/card/card-with-title.html.twig" with {contentSpacing: false} %}
    {% import "system/macro/card/card-heading.html.twig" as card_heading %}
    {% block header %}
        {{ card_heading.component({
            title: localized.headline.acknowledgement,
        }) }}
    {% endblock %}
    {% block content %}
        {% embed "system/embed/form-row.html.twig" %}
            {% block label %}
                <div class="bc-flex bc-items-center">
                    <span>Animate.css</span>
                </div>
            {% endblock %}

            {% block input %}
                <a class="brlbs-cmpnt-link brlbs-cmpnt-link-with-icon" href="https://animate.style/" rel="nofollow noopener noreferrer" target="_blank">
                    <span>https://animate.style/</span>
                    <span class="brlbs-cmpnt-external-link-icon"></span>
                </a>
            {% endblock %}
        {% endembed %}

        {% embed "system/embed/form-row.html.twig" %}
